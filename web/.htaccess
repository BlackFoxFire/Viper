DirectoryIndex app.php
Options Indexes FollowSymlinks

<IfModule mod_negotiation.c>
    Options -MultiViews
</IfModule>

<IfModule mod_rewrite.c>
	RewriteEngine On
	
	RewriteBase /dev/Viper/web/
	
	RewriteCond %{REQUEST_FILENAME} -f
	RewriteRule ^ - [L]
	
	RewriteRule ^(.*)$ app.php [QSA,L]
	
</IfModule>